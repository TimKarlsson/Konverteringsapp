<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tim_tub.konverteringsapp">

    <!--<uses-permission android:name="android.permission.WAKE_LOCK" />-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".converterActivity_choise"
            android:label=".converterActivity_choise"
            android:screenOrientation="portrait" />
        <activity
            android:name=".converterActivity_core"
            android:label=".converterActivity_core"
            android:parentActivityName=".converterActivity_choise">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="tim_tub.konverteringsapp.converterActivity_choise" />
        </activity>
        <activity
            android:name=".infoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".infoActivity_moment"
            android:parentActivityName=".infoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tim_tub.konverteringsapp.infoActivity" />
        </activity>
        <activity
            android:name=".infoActivity_qualityInfo"
            android:label=".infoActivity_qualityInfo"
            android:parentActivityName=".infoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tim_tub.konverteringsapp.infoActivity" />
        </activity>
    </application>

</manifest>